        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CasusBlok2Main.Views"
        mc:Ignorable="d"
        Title="MedewerkerPanel" Height="219" Width="389">
    <Grid>
        <Button x:Name="KlachtenKnop" Content="Openstaande&#xD;&#xA;klachten" HorizontalAlignment="Left" VerticalAlignment="Top" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" Margin="57,45,0,0" Height="40" Click="OpenKlachten"/>
        <Button x:Name="Belmomenteknop" Content="Geplande&#xD;&#xA;belmomenten" HorizontalAlignment="Left" VerticalAlignment="Top" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" Margin="204,45,0,0" Height="40" Click="Belmomenteknop_Click"/>
