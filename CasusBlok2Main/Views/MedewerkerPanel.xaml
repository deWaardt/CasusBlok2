        <Button x:Name="KlantInfoKnop" Content="Zie Klantinfo" HorizontalAlignment="Left" Margin="57,92,0,0" VerticalAlignment="Top" Width="100" Height="38" Click="KlantInfoKnop_Click"/>
    </Grid>
</Window>
