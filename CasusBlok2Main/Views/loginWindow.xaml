        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CasusBlok2Main.Views"
        mc:Ignorable="d"
        Title="loginWindow" Height="300" Width="300">
    <Grid>
        <TextBox HorizontalAlignment="Left" x:Name="usrname" Height="23" Margin="83,89,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
